<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="fos_user_profile_show_list" path="/users/list" methods="GET">
        <default key="_controller">SonataUserBundle:Users:list</default>
    </route>

    <route id="fos_user_profile_show_single" path="/users/list/{id}" methods="GET">
        <default key="_controller">SonataUserBundle:Users:singleShow</default>
    </route>

</routes>
